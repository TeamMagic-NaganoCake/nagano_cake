<%= render "layouts/errors", obj: @genre %>
<h4 class="mt-5 mb-5">ジャンル編集</h4>
<%= form_with model: @genre, url: admin_genre_path do |f| %>
  <%= f.label :genre_name, "ジャンル名", class: "small font-weight-bold mr-5" %>
  <%= f.text_field :genre_name, class: "mr-5" %>
  <%= f.submit "変更を保存", class: "btn btn-success"%>
<% end %>